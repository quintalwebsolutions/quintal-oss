{
  "name": "@quintal/result",
  "version": "0.2.0",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "run-s build:*",
    "build:clean": "shx rm -rf dist",
    "build:code": "vite build",
    "clean": "shx rm -rf .config .coverage .coverage-ts .turbo dist node_modules",
    "dev": "vitest --watch",
    "lint": "pnpm lint:fix && pnpm lint:types",
    "lint:check": "biome ci .",
    "lint:fix": "biome check --apply-unsafe .",
    "lint:types": "tsc --noEmit",
    "test": "run-p test:*",
    "test:source": "vitest",
    "test:types": "typescript-coverage-report --outputDir .coverage-ts --strict"
  },
  "devDependencies": {
    "@quintal/config": "workspace:*",
    "@types/node": "20.10.2",
    "@vitest/coverage-v8": "0.34.6",
    "happy-dom": "12.10.3",
    "typescript": "5.3.2",
    "vite": "5.0.4",
    "vitest": "0.34.6"
  }
}
