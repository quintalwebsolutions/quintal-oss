{
  "name": "@quintal/monads",
  "version": "0.0.1",
  "main": "./.dist/index.cjs",
  "module": "./.dist/index.mjs",
  "types": "./.dist/index.d.ts",
  "scripts": {
    "build": "run-s build:*",
    "build:clean": "shx rm -rf .dist",
    "build:code": "vite build",
    "clean": "shx rm -rf .config .coverage .coverage-ts .dist .docs .turbo node_modules",
    "dev": "vitest --watch",
    "doc": "typedoc src/index.ts --plugin typedoc-plugin-markdown --out ./.docs --readme ./README.md ",
    "lint": "pnpm lint:fix && pnpm lint:types",
    "lint:check": "biome ci .",
    "lint:fix": "biome check --apply-unsafe .",
    "lint:types": "tsc --noEmit",
    "test": "run-s test:*",
    "test:source": "vitest",
    "test:types": "typescript-coverage-report --outputDir .coverage-ts --strict"
  },
  "devDependencies": {
    "@quintal/config": "workspace:*",
    "@types/node": "20.10.2",
    "@vitest/coverage-v8": "0.34.6",
    "happy-dom": "12.10.3",
    "typedoc": "0.25.7",
    "typedoc-plugin-markdown": "3.17.1",
    "typescript": "5.3.2",
    "typescript-coverage-report": "0.8.0",
    "vite": "5.0.4",
    "vitest": "0.34.6"
  }
}
